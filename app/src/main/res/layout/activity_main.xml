<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/aMain_welcome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="WelcomeActivity"
        />
    <Button
        android:id="@+id/aMain_settings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SettingsActivity"/>
    <Button
        android:id="@+id/aMain_library"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="LibraryActivity"/>
    <Button
        android:id="@+id/aMain_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="UserActivity"/>

    <Button
        android:id="@+id/aMain_exercise"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ExerciseActivity"/>

</LinearLayout>